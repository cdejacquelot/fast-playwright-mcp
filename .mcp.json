{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "fast-playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "fast-playwright-mcp"
      ],
      "env": {}
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "/Users/tomohiko/fast-playwright-mcp",
        "--enable-web-dashboard",
        "False"
      ],
      "env": {}
    }
  }
}
